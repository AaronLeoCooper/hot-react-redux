{
  "name": "reredux",
  "version": "1.0.0",
  "description": "React Redux starting point",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "start": "better-npm-run start",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "test": "better-npm-run ",
    "test:watch": "better-npm-run "
  },
  "betterScripts": {
    "start": {
      "command": "nodemon --exec 'gulp'",
      "env": {
        "NODE_ENV": "development",
        "HMR": "true"
      }
    },
    "build:dev": {
      "command": "gulp build:dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "gulp build:prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "jest --coverage",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "jest --watch",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "Aaron Leo Cooper",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "better-npm-run": "0.0.11",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.1.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.10",
    "immutable": "^3.8.1",
    "node-sass": "^3.10.1",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "run-sequence": "^1.2.2",
    "webpack": "^1.13.2",
    "webpack-strip": "^0.1.0"
  },
  "devDependencies": {
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.16.0",
    "eslint": "^3.6.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "jest": "^15.1.1",
    "nodemon": "^1.10.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "webpack-dev-server": "^1.16.1"
  }
}
